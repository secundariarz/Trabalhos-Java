package ExercÃ­cios;

import java.util.Scanner;

public class Ex13 {
	public static void main(String[] args) {
		Scanner entrada = new Scanner(System.in);
		
		System.out.println("DIGITA O PRIMEIRO: ");
		int value = entrada.nextInt();
		
		System.out.println("DIGITA O SEGUNDO: ");
		int value2 = entrada.nextInt();
		
		System.out.println("DIGITA O TERCEIRO: ");
		int value3 = entrada.nextInt();
		
		int[] myNum = {value , value2 , value3};
		
		for(int i : myNum) {
			if(myNum[0] > myNum[1] && myNum[0] > myNum[2]) {
				System.out.println(myNum[0]);
			} else if(myNum[1] > myNum[0] && myNum[1] > myNum[2]) {
				System.out.println(myNum[1]);
			} else if(myNum[2] > 1 && myNum[2] > myNum[0]) {
				System.out.println(myNum[2]);
			}
		}
		
		
	}
	
	
}
